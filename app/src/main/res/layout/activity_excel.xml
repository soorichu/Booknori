<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="co.soori.booknori.ExcelActivity">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">
        <Button
            android:id="@+id/text_read"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="text read"/>
        <Button
            android:id="@+id/text_write"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="text write"/>
        <Button
            android:id="@+id/excel_read"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="excel read"/>
        <Button
            android:id="@+id/excel_write"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="excel write"/>

    </LinearLayout>

</android.support.constraint.ConstraintLayout>
